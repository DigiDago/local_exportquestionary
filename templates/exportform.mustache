<div class="exportform text-center">
    <div class="loadercontainer hide">
        <div class="loader"></div>
    </div>
    <div class="form-row">
        <div class="form-group ml-auto">
            <select name="templatetitle" id="templatetitle" class="form-control" aria-label="template questionary">
                {{#templatequestionary}}
                    <option value="{{{title}}}">{{{title}}}</option>
                {{/templatequestionary}}
            </select>
        </div>
        <div class="form-group mr-auto">
            <select name="exportformat" id="exportformat" class="form-control" aria-label="export format">
                <option value="csv">csv</option>
                <option value="xlsx">xlsx</option>
            </select>
        </div>
    </div>
